{
    "name": "influendo/laravel-survivor",
    "description": "Doesn't let your tokens expire",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Strahija",
            "email": "boris@creolab.hr"
        },
        {
            "name": "Influendo",
            "email": "influendo@influendo.hr"
        },
        {
            "name": "Syahril Zulkefli",
            "email": "syahzul@me.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Influendo\\LaravelSurvivor\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "illuminate/support": "~6.0",
        "illuminate/routing": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Influendo\\LaravelSurvivor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Influendo\\LaravelSurvivor\\SurvivorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
